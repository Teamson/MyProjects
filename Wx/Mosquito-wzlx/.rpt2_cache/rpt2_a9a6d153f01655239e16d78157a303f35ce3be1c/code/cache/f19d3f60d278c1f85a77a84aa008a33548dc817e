i.WxOnShow(WxApi.killbossCallback);\r\n        Laya.timer.frameLoop(1, this, this.myUpdate);\r\n    }\r\n    onClosed() {\r\n        AdMgr.instance.hideBanner();\r\n        Laya.timer.clearAll(this);\r\n        if (this.closeCallback) {\r\n            Laya.timer.once(100, this, () => {\r\n                this.closeCallback();\r\n            });\r\n        }\r\n        WxApi.isKillBossUI = false;\r\n    }\r\n    decBar() {\r\n        this.bar.value -= JJMgr.instance.dataConfig.front_progress_bar_return;\r\n        if (this.bar.value < 0) {\r\n            this.bar.value = 0;\r\n        }\r\n        this.panelBar.y = 277 - this.bar.value * 277;\r\n    }\r\n    clickBtnCBDown() {\r\n        this.bar.value += this.addProgerss;\r\n        this.panelBar.y = 277 - this.bar.value * 277;\r\n        if (this.bar.value >= this.needClick * this.addProgerss && !this.hadShowBanner) {\r\n            this.hadShowBanner = true;\r\n            WxPlatformBannerAd.Instance.catchErrorCB = () => { Laya.Scene.close('MyScenes/MadUI.scene'); };\r\n            if (!WxPlatformBannerAd.Instance.isLoadBannerError) {\r\n                AdMgr.instance.showBanner();\r\n            }\r\n            else {\r\n                CustomAd.instance.showBottomAd();\r\n            }\r\n            Laya.timer.once(2500, this, () => {\r\n                if (this.isforce) {\r\n                    if (WxPlatformBannerAd.Instance.isLoadBannerError && CustomAd.instance.isBottomAdError) {\r\n                        this.closeBtnCB();\r\n                    }\r\n                    if (!WxPlatformBannerAd.Instance.isLoadBannerError) {\r\n                        AdMgr.instance.hideBanner();\r\n                    }\r\n                    else {\r\n                        CustomAd.instance.hideBottomAd();\r\n                    }\r\n                    this.hadShowBanner = false;\r\n                }\r\n                else {\r\n                    this.closeBtnCB();\r\n                }\r\n            });\r\n        }\r\n        JJMgr.instance.JJSendEvent('宝箱页猛点按钮');\r\n        this.clickBtn.scaleX = 1.1;\r\n        this.clickBtn.scaleY = 1.1;\r\n    }\r\n    timeCounter() {\r\n        this.clickCount = 0;\r\n    }\r\n    clickBtnCBUp() {\r\n        this.clickBtn.scaleX = 1;\r\n        this.clickBtn.scaleY = 1;\r\n    }\r\n    closeBtnCB() {\r\n        this.close();\r\n    }\r\n    myUpdate() {\r\n    }\r\n}\r\n//# sourceMappingURL=MadUI.js.map",
