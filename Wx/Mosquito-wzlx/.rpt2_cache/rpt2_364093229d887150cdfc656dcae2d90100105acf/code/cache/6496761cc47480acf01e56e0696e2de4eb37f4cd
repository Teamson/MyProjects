Share.gameStart(); });\r\n    }\r\n    skinBtnCB() {\r\n        JJWxAd.Instance.hideBanner();\r\n        JJWxAd.Instance.hideHomeGridAd();\r\n        Laya.Scene.open('MyScenes/SkinUI.scene');\r\n        GameLogic.Share._scene.active = false;\r\n        GameLogic.Share._camera.active = false;\r\n    }\r\n    mapBtnCB() {\r\n    }\r\n    moreGameBtnCB() {\r\n        JJWxAd.Instance.hideBanner();\r\n        JJWxAd.Instance.hideHomeGridAd();\r\n        JJWxTrap.Instance.showFullGameUI(() => {\r\n            JJWxAd.Instance.showHomeGridAd();\r\n        });\r\n    }\r\n    updateCB() {\r\n    }\r\n}\r\n//# sourceMappingURL=StartUI.js.map",
