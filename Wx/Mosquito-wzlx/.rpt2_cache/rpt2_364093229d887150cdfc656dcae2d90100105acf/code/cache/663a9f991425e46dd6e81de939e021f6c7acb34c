ay.length) {\r\n            return;\r\n        }\r\n        var item = cell.getChildByName('item');\r\n        var icon = item.getChildByName('icon');\r\n        var name = item.getChildByName('name');\r\n        icon.skin = this.navData1[index].icon;\r\n        name.text = JJMgr.instance.getTitle(this.navData1[index]);\r\n        item.off(Laya.Event.CLICK, this, this.navCB, [this.navData1, index]);\r\n        item.on(Laya.Event.CLICK, this, this.navCB, [this.navData1, index]);\r\n    }\r\n    initList2() {\r\n        this.navList2.x = this.width - 335;\r\n        this.navData2 = [].concat(JJMgr.instance.navDataArr);\r\n        this.navData2 = JJMgr.instance.shuffleArr(this.navData2);\r\n        this.navList2.vScrollBarSkin = '';\r\n        this.navList2.repeatX = 2;\r\n        this.navList2.repeatY = this.navData2.length / 2;\r\n        this.navList2.array = this.navData2;\r\n        this.navList2.renderHandler = Laya.Handler.create(this, this.onListRender2, null, false);\r\n        this.navList2.mouseHandler = new Laya.Handler(this, this.mouseHandler2);\r\n        this.scrollLoop2();\r\n    }\r\n    mouseHandler2(e, index) {\r\n        Laya.timer.clear(this, this.scrollLoop2);\r\n        Laya.timer.once(1000, this, this.scrollLoop2);\r\n    }\r\n    scrollLoop2() {\r\n        Laya.timer.once(1000, this, () => {\r\n            let num = Math.floor(this.navList2.startIndex);\r\n            if (this.scrollDir2 == 1) {\r\n                num += 2;\r\n                if (num >= this.navData2.length - 6) {\r\n                    num--;\r\n                    this.scrollDir2 = -this.scrollDir2;\r\n                }\r\n            }\r\n            else {\r\n                num -= 2;\r\n                if (num < 0) {\r\n                    this.scrollDir2 = -this.scrollDir2;\r\n                }\r\n            }\r\n            this.navList2.tweenTo(num, 1000, Laya.Handler.create(this, this.scrollLoop2));\r\n        });\r\n    }\r\n    onListRender2(cell, index) {\r\n      