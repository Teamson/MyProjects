his, () => {\r\n                JJMgr.instance.openScene(SceneDir.SCENE_FULLGAMEUI, false, {\r\n                    continueCallbackFun: () => {\r\n                        JJMgr.instance.openScene(SceneDir.SCENE_RECOMMENDUI, false, {\r\n                            closeCallbackFun: () => {\r\n                                JJMgr.instance.openScene(SceneDir.SCENE_FRIENDGAME, false, {\r\n                                    closeCallbackFun: () => {\r\n                                        JJMgr.instance.JJSendEvent('进入首页');\r\n                                        Laya.timer.once(100, this, () => {\r\n                                            AdMgr.instance.showBanner();\r\n                                        });\r\n                                    }\r\n                                });\r\n                            }\r\n                        });\r\n                    }\r\n                });\r\n            });\r\n        }\r\n        else {\r\n            AdMgr.instance.showBanner();\r\n        }\r\n    }\r\n    onClosed() {\r\n        Laya.timer.clearAll(this);\r\n    }\r\n    hideExportBtnCB() {\r\n        this.hideExportTimeCounter++;\r\n        if (JJMgr.instance.dataConfig.front_all_export_switch && this.hideExportTimeCounter >= 10) {\r\n            JJMgr.instance.dataConfig.front_all_export_switch = false;\r\n            JJMgr.instance.dataConfig.is_allow_area = false;\r\n            WxApi.OpenAlert('纯净模式已开启！');\r\n            Laya.Scene.open('MyScenes/StartUI.scene');\r\n        }\r\n    }\r\n    startBtnCB() {\r\n        JJMgr.instance.JJSendEvent('开始游戏');\r\n        SoundMgr.instance.playSoundEffect('Click.mp3');\r\n        if (this.hadStart)\r\n            return;\r\n        this.hadStart = true;\r\n        let cb = () => {\r\n            if (!AdMgr.instance.isBannerError && WxApi.isValidBanner() && JJMgr.instance.dataConfig.front_box_switch) {\r\n                Laya.Scene.open('MyScenes/MadUI.scene', true, {\r\n                    closeCallback: () => {\r\n                        if (JJMgr.instance.dataConfig.front_all_screen_page) {\r\n                            JJMgr.instance.openScene(SceneDir.SCENE_FULLGAMEUI, true, {\r\n                                continueCallbackFun: () => {\r\n                                    GameLogic.Share.gameStart();\r\n                                }\r\n                            });\r\n                        }\r\n                        else {\r\n                            GameLogic.Share.gameStart();\r\n                        }\r\n                    }\r\n                });\r\n            }\r\n            else {\r\n                GameLogic.Share.gameStart();\r\n            }\r\n        };\r\n        if (!WxApi.firstStartGame && JJMgr.instance.dataConfig.front_start_game_switch && !AdMgr.instance.videoIsError && WxApi.isValidBanner()) {\r\n            WxApi.firstStartGame = true;\r\n            AdMgr.instance.adCloseCallback = cb;\r\n            AdMgr.instance.showVideo(() => { });\r\n        }\r\n        else {\r\n            cb();\r\n        }\r\n    }\r\n    skinBtnCB() {\r\n        AdMgr.instance.hideBanner();\r\n        SoundMgr.instance.playSoundEffect('Click.mp3');\r\n        GameLogic.Share._camera.active = false;\r\n        Laya.Scene.open('MyScenes/SkinUI.scene');\r\n    }\r\n    moreGameBtnCB() {\r\n        JJMgr.instance.JJSendEvent('点我有惊喜');\r\n        SoundMgr.instance.playSoundEffect('Click.mp3');\r\n        AdMgr.instance.hideBanner();\r\n        JJMgr.instance.openScene(SceneDir.SCENE_FULLGAMEUI, false, {\r\n            continueCallbackFun: () => {\r\n                JJMgr.instance.openScene(SceneDir.SCENE_DRAWUI, false, {\r\n                    autoTime: 1500,\r\n                    closeCallbackFun: () => { this.drawValid = true; }\r\n                });\r\n                AdMgr.instance.showBanner();\r\n            }\r\n        });\r\n    }\r\n    drawBtnCB() {\r\n        SoundMgr.instance.playSoundEffect('Click.mp3');\r\n        if (!this.drawValid)\r\n            return;\r\n        this.drawValid = false;\r\n        JJMgr.instance.openScene(SceneDir.SCENE_DRAWUI, false, { closeCallbackFun: () => { this.drawValid = true; } });\r\n        WxApi.aldEvent('点击抽屉按钮人数');\r\n    }\r\n    updateCB() {\r\n        this.coinNum.value = PlayerDataMgr.getPlayerData().coin.toString();\r\n    }\r\n}\r\n//# sourceMappingURL=StartUI.js.map",
