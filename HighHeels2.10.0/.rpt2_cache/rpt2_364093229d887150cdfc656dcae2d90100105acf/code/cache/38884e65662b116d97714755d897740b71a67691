  "code": "import GameConfig from \"../../GameConfig\";\r\nimport JJMgr from \"./JJMgr\";\r\nexport default class JJWxBannerAd {\r\n    constructor() {\r\n        this.showBanner = () => { };\r\n        this.hideBanner = () => { };\r\n        this.currentIndex = 0;\r\n        this.canLoadAgain = true;\r\n    }\r\n    static get Instance() {\r\n        if (!JJWxBannerAd._instance) {\r\n            JJWxBannerAd._instance = new JJWxBannerAd;\r\n        }\r\n        return JJWxBannerAd._instance;\r\n    }\r\n    get sysInfo() {\r\n        if (!this._sysInfo) {\r\n            this._sysInfo = wx[\"getSystemInfoSync\"]();\r\n        }\r\n        return this._sysInfo;\r\n    }\r\n    initAd(isAllowArea = false, isAllowScene = false, bannerCount = 5, changeTime = 30000, refreshNum = 1, bannerNum = 1, bannerIds = []) {\r\n        this.bannerIds = [\"adunit-1f7179c2cd82df22\"];\r\n        if (isAllowArea && isAllowScene) {\r\n            console.log(\"新机制================\");\r\n            this.changeTime = changeTime;\r\n            this.refreshNum = refreshNum;\r\n            this.bannerNum = bannerNum;\r\n            this.bannerIds = this.bannerIds.concat(bannerIds);\r\n            this.useInnovation = true;\r\n            this.loadBannerNew();\r\n            this.showBanner = this.showBannerNew;\r\n            this.hideBanner = this.hideBannerNew;\r\n        }\r\n        else {\r\n            this.bannerCount = bannerCount;\r\n            this.currentCount = bannerCount;\r\n            this.useInnovation = false;\r\n            this.loadBannerOld();\r\n            this.showBanner = this.showBannerOld;\r\n            this.hideBanner = this.hideBannerOld;\r\n        }\r\n    }\r\n    getBannerId() {\r\n        var randomIndex = Math.floor(Math.random() * 100) % this.bannerIds.length;\r\n        this.currentBannerId = this.bannerIds[randomIndex];\r\n        return this.bannerIds[randomIndex];\r\n    }\r\n    setBannerTop(top, left) {\r\n        if (!this.currentBanner)\r\n            return;\r\n        let sysInfo = this.sysInfo;\r\n        if (this.useInnovation) {\r\n            var bannerAd = this.currentBanner.banner;\r\n        }\r\n        else {\r\n            bannerAd = this.currentBanner;\r\n        }\r\n        let realHeight = bannerAd.style.realHeight + 0.1;\r\n        if (sysInfo.model.startsWith(\"iPhone X\"))\r\n            realHeight += (40 / Laya.Browser.height) * GameConfig.height;\r\n        bannerAd.style.top = sysInfo.screenHeight - realHeight + top;\r\n        if (left != null) {\r\n            bannerAd.style.left = left;\r\n        }\r\n    }\r\n    get getBannerAd() {\r\n        return this.useInnovation ? this.currentBanner.banner : this.currentBanner;\r\n    }\r\n    loadBannerOld() {\r\n        if (!Laya.Browser.onWeiXin)\r\n            return;\r\n        if (this.isLoading)\r\n            return;\r\n        this.isLoading = true;\r\n        if (this.currentBanner) {\r\n            this.currentBanner.destroy();\r\n            this.currentBanner = null;\r\n        }\r\n        let sysInfo = this.sysInfo;\r\n        let id = this.getBannerId();\r\n        let bannerAd = wx[\"createBannerAd\"]({\r\n            adUnitId: id,\r\n            style: {\r\n                top: sysInfo.screenHeight * 0.8,\r\n                width: 300,\r\n                left: sysInfo.screenWidth / 2 - 150\r\n            }\r\n        });\r\n        bannerAd.onLoad(() => {\r\n            console.log(\"create banner success\");\r\n            JJWxBannerAd.Instance.currentBanner = bannerAd;\r\n            JJWxBannerAd.Instance.isLoading = false;\r\n            JJWxBannerAd.Instance.isLoadBannerError = false;\r\n            if (JJWxBannerAd.Instance.isloadAndShow) {\r\n                JJWxBannerAd.Instance.isloadAndShow = false;\r\n                JJWxBannerAd.Instance.showBannerOld();\r\n            }\r\n        });\r\n        bannerAd.onError((err) => {\r\n            console.log(\"create banner error\", err);\r\n            JJWxBannerAd.Instance.isLoading = false;\r\n            JJWxBannerAd.Instance.isLoadBannerError = true;\r\n            JJMgr.instance.eventDispatcher.event(\"hide_box_scene\");\r\n        });\r\n        bannerAd.onResize((res) => {\r\n            let realHeight = bannerAd.style.realHeight + 0.1;\r\n            if (sysInfo.model.startsWith(\"iPhone X\"))\r\n                realHeight += (40 / Laya.Browser.height) * GameConfig.height;\r\n            bannerAd.style.top = sysInfo.screenHeight - realHeight;\r\n        });\r\n    }\r\n    showBannerOld() {\r\n        if (Laya.Browser.onWeiXin) {\r\n            if (!this.currentBanner) {\r\n                this.isloadAndShow = true;\r\n                this.loadBannerOld();\r\n                return;\r\n            }\r\n            if (!this.currentBanner)\r\n                return;\r\n            if (this.isShowBanner)\r\n                return;\r\n            this.isShowBanner = true;\r\n            let banner = this.currentBanner;\r\n            banner.show();\r\n            console.log(\"show banner\");\r\n            this.setBannerTop(0);\r\n        }\r\n        else {\r\n            if (this.isShowBanner)\r\n                return;\r\n            this.isShowBanner = true;\r\n            this.showVirtualBanner();\r\n        }\r\n    }\r\n    hideBannerOld() {\r\n        if (Laya.Browser.onWeiXin) {\r\n            this.isloadAndShow = false;\r\n            if (this.isShowBanner == false)\r\n                return;\r\n            this.isShowBanner = false;\r\n            if (!this.currentBanner)\r\n                return;\r\n            this.currentBanner.hide();\r\n            console.log(\"hide banner\", this.isShowBanner, this.currentBanner);\r\n            this.countBanner();\r\n        }\r\n        else {\r\n            if (this.isShowBanner == false)\r\n                return;\r\n            this.isShowBanner = false;\r\n            this.hideVirtualBanner();\r\n        }\r\n    }\r\n    countBanner() {\r\n        if (!this.currentBanner)\r\n            return;\r\n        if (this.currentCount == 0) {\r\n            this.currentCount = this.bannerCount;\r\n            this.loadBannerOld();\r\n        }\r\n        this.currentCount--;\r\n        console.log(\"banner剩余展示次数\", this.bannerCount);\r\n    }\r\n    loadBannerNew() {\r\n        if (!Laya.Browser.onWeiXin)\r\n            return;\r\n        if (!this.canLoadAgain)\r\n            return;\r\n        this.canLoadAgain = false;\r\n        console.log(\"create banner\");\r\n        this.banners = [];\r\n        let sysInfo = this.sysInfo;\r\n        let count = this.bannerIds.length * this.bannerNum;\r\n        let errorCount = count;\r\n        let totalCount = count;\r\n        for (let i = 0; i < this.bannerIds.length; i++) {\r\n            let id = this.bannerIds[i];\r\n            for (let j = 0; j < this.bannerNum; j++) {\r\n                let bannerAd = wx[\"createBannerAd\"]({\r\n                    adUnitId: id,\r\n                    style: {\r\n                        top: sysInfo.screenHeight * 0.8,\r\n                        width: 300,\r\n                        left: sysInfo.screenWidth / 2 - 150\r\n                    }\r\n                });\r\n                bannerAd.onLoad(() => {\r\n                    let item = { banner: bannerAd, showCount: 0 };\r\n                    JJWxBannerAd.Instance.banners.push(item);\r\n                    JJWxBannerAd.Instance.isLoadBannerError = false;\r\n                    if (--count == 0) {\r\n                        if (JJWxBannerAd.Instance.isloadAndShow) {\r\n                            JJWxBannerAd.Instance.showBanner();\r\n                        }\r\n                        console.log(\"front_banner_ids=========\", this.banners);\r\n                    }\r\n                    if (--totalCount == 0) {\r\n                        JJWxBannerAd.Instance.canLoadAgain = true;\r\n                    }\r\n                });\r\n                bannerAd.onError((err) => {\r\n                    console.log(\"create banner error\", err);\r\n                    if (--errorCount == 0) {\r\n                        JJWxBannerAd.Instance.isLoadBannerError = true;\r\n                        JJMgr.instance.eventDispatcher.event(\"hide_box_scene\");\r\n                    }\r\n                    if (--totalCount == 0) {\r\n                        JJWxBannerAd.Instance.canLoadAgain = true;\r\n                    }\r\n                });\r\n                bannerAd.onResize((res) => {\r\n                    let realHeight = bannerAd.style.realHeight + 0.1;\r\n                    if (sysInfo.model.startsWith(\"iPhone X\"))\r\n                        realHeight += (40 / Laya.Browser.height) * GameConfig.height;\r\n                    bannerAd.style.top = sysInfo.screenHeight - realHeight;\r\n                });\r\n            }\r\n        }\r\n    }\r\n    showBannerNew() {\r\n        if (Laya.Browser.onWeiXin) {\r\n            this.hideBannerNew();\r\n            if (!this.banners || this.banners.length == 0) {\r\n                this.loadBannerNew();\r\n                this.isloadAndShow = true;\r\n                return;\r\n            }\r\n            if (this.currentIndex >= this.banners.length)\r\n                this.currentIndex = 0;\r\n            let item = this.banners[this.currentIndex];\r\n            Laya.timer.once(this.changeTime, this, this.showBannerNew);\r\n            if (!item || !item.banner)\r\n                return;\r\n            this.currentIndex++;\r\n            item.banner.show();\r\n            item.showCount++;\r\n            this.currentBanner = item;\r\n            console.log(\"show banner\", this.currentIndex - 1);\r\n            this.setBannerTop(0);\r\n        }\r\n        else {\r\n            this.showVirtualBanner();\r\n        }\r\n    }\r\n    hideBannerNew() {\r\n        this.isloadAndShow = false;\r\n        if (Laya.Browser.onWeiXin) {\r\n            let item = this.currentBanner;\r\n            if (!item)\r\n                return;\r\n            item.banner.hide();\r\n            if (item.showCount >= this.refreshNum) {\r\n                item.banner.destroy();\r\n                this.banners.splice(this.banners.indexOf(item), 1);\r\n                console.log(\"destroy banner\");\r\n            }\r\n            console.log(\"hide banner\", this.currentIndex - 1);\r\n            this.currentBanner = null;\r\n            Laya.timer.clear(this, this.showBannerNew);\r\n        }\r\n        else {\r\n            this.hideVirtualBanner();\r\n        }\r\n    }\r\n    showVirtualBanner() {\r\n        if (!this.bannerImage) {\r\n            var scene = new Laya.Scene;\r\n            Laya.stage.addChild(scene);\r\n            scene.zOrder += 20;\r\n            scene.size(Laya.stage.width, Laya.stage.height);\r\n            this.bannerImage = new Laya.Image;\r\n            this.bannerImage.skin = \"JJExportRes/ex (7).png\";\r\n            scene.addChild(this.bannerImage);\r\n            this.bannerImage.centerX = 0;\r\n            this.bannerImage.bottom = 0;\r\n            this.bannerImage.size(750 * 0.7, 200);\r\n        }\r\n        this.bannerImage.visible = true;\r\n    }\r\n    hideVirtualBanner() {\r\n        if (!this.bannerImage)\r\n            return;\r\n        this.bannerImage.visible = false;\r\n    }\r\n}\r\n//# sourceMappingURL=JJWxBannerAd.js.map",
