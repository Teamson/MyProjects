  "code": "import WxApi from \"../Libs/WxApi\";\r\nimport PlayerDataMgr from \"../Libs/PlayerDataMgr\";\r\nimport Utility from \"../Mod/Utility\";\r\nimport Player from \"./Player\";\r\nimport SoundMgr from \"../Mod/SoundMgr\";\r\nimport JJMgr, { SceneDir } from \"../JJExport/Common/JJMgr\";\r\nimport PlayerBall from \"./PlayerBall\";\r\nimport Ball from \"./Ball\";\r\nimport GameUI from \"../View/GameUI\";\r\nimport End from \"./End\";\r\nexport default class GameLogic {\r\n    constructor() {\r\n        this.camStartPos = new Laya.Vector3(0, 0, 0);\r\n        this.camStartRotation = null;\r\n        this.lightStartForward = null;\r\n        this.planePos = null;\r\n        this._levelNode = null;\r\n        this._roadNode = null;\r\n        this._player = null;\r\n        this._playerCrl = null;\r\n        this._playerBall = null;\r\n        this._playerBallCrl = null;\r\n        this.powerNum = 0;\r\n        this.totalDistance = 0;\r\n        this.isDes = false;\r\n        this.hadKicked = false;\r\n        this.colorId = 0;\r\n        this.isStartGame = false;\r\n        this.isGameOver = false;\r\n        this.isWin = false;\r\n        this.isPause = false;\r\n        localStorage.clear();\r\n        GameLogic.Share = this;\r\n        PlayerDataMgr.getPlayerData();\r\n        Laya.Scene.open('MyScenes/LoadingUI.scene');\r\n    }\r\n    initScene() {\r\n        Laya.Scene3D.load(WxApi.UnityPath + 'SampleScene.ls', Laya.Handler.create(this, this.onLoadScene));\r\n    }\r\n    onLoadScene(scene) {\r\n        Laya.Scene.open('MyScenes/StartUI.scene');\r\n        WxApi.aldEvent('进入首页');\r\n        this._scene = Laya.stage.addChild(scene);\r\n        Laya.stage.setChildIndex(this._scene, 0);\r\n        this._camera = this._scene.getChildByName('Main Camera');\r\n        this._light = this._scene.getChildByName('Directional Light');\r\n        this._light.transform.worldMatrix.setForward(new Laya.Vector3(1, -1, 1));\r\n        this.fixCameraField();\r\n        this.camStartPos = this._camera.transform.position.clone();\r\n        this.camStartRotation = this._camera.transform.rotation.clone();\r\n        this._levelNode = new Laya.Sprite3D();\r\n        this._scene.addChild(this._levelNode);\r\n        this.createLevel();\r\n    }\r\n    fixCameraField() {\r\n        let staticDT = 1624 - 1334;\r\n        let curDT = Laya.stage.displayHeight - 1334 < 0 ? 0 : Laya.stage.displayHeight - 1334;\r\n        let per = curDT / staticDT * 10;\r\n        this._camera.fieldOfView = 90 + per;\r\n    }\r\n    setFog() {\r\n        let scene = this._scene;\r\n        scene.enableFog = true;\r\n        scene.fogColor = this.getRGB(\"#4DC4FF\");\r\n        scene.fogStart = 8;\r\n        scene.fogRange = 25;\r\n    }\r\n    getRGB(_hexColor) {\r\n        var color = [], rgb = [];\r\n        let hexColor = _hexColor.replace(/#/, \"\");\r\n        if (hexColor.length == 3) {\r\n            var tmp = [];\r\n            for (var i = 0; i < 3; i++) {\r\n                tmp.push(hexColor.charAt(i) + hexColor.charAt(i));\r\n            }\r\n            hexColor = tmp.join(\"\");\r\n        }\r\n        for (var i = 0; i < 3; i++) {\r\n            color[i] = \"0x\" + hexColor.substr(i * 2, 2);\r\n            rgb.push(parseInt(color[i]));\r\n        }\r\n        return new Laya.Vector3(rgb[0] / 255, rgb[1] / 255, rgb[2] / 255);\r\n    }\r\n    createLevel() {\r\n        this._roadNode = new Laya.Sprite3D();\r\n        this._levelNode.addChild(this._roadNode);\r\n        this._player = Laya.Sprite3D.instantiate(Utility.getSprite3DResByUrl('Hero.lh'), this._levelNode, false, new Laya.Vector3(0, 1, -3));\r\n        this._playerCrl = this._player.addComponent(Player);\r\n        let randId = Math.floor(Math.random() * 3);\r\n        this.colorId = randId;\r\n        this._playerCrl.setMyColor(randId);\r\n        this._playerBall = Laya.Sprite3D.instantiate(Utility.getSprite3DResByUrl('PlayerBall.lh'), this._levelNode, false, new Laya.Vector3(0, 1, -2.4));\r\n        this._playerBallCrl = this._playerBall.addComponent(PlayerBall);\r\n        this._playerBallCrl.setMyColor(randId);\r\n        for (let i = 0; i < 8; i++) {\r\n            let road = Laya.Sprite3D.instantiate(Utility.getSprite3DResByUrl('Road.lh'), this._roadNode, false, new Laya.Vector3(0, 0, 0));\r\n            road.transform.position = new Laya.Vector3(0, 0, i * 16);\r\n            if (i >= 1) {\r\n                this.createBall(road);\r\n                if (Math.floor(i % 2) != 0) {\r\n                    randId = Math.floor(Math.random() * 3);\r\n                    this.createColorBoard(road, randId);\r\n                }\r\n            }\r\n            road.name = randId.toString();\r\n        }\r\n        this._finish = Laya.Sprite3D.instantiate(Utility.getSprite3DResByUrl('End.lh'), this._levelNode, false, new Laya.Vector3(0, 0, 0));\r\n        this._finish.transform.position = new Laya.Vector3(0, 0, (this._roadNode.numChildren - 1) * 16);\r\n        this._finish.addComponent(End);\r\n        this.totalDistance = (this._roadNode.numChildren - 1) * 16;\r\n    }\r\n    createBall(road) {\r\n        let arr = [0, 1, 2];\r\n        arr = Utility.shuffleArr(arr);\r\n        for (let i = 0; i < 6; i++) {\r\n            if (i >= 3)\r\n                continue;\r\n            let ball = Laya.Sprite3D.instantiate(Utility.getSprite3DResByUrl('Ball.lh'), road, false, new Laya.Vector3(0, 0, 0));\r\n            let crl = ball.addComponent(Ball);\r\n            let x = 2.2 - Math.floor(i % 3) * 2.2;\r\n            let z = -5 + Math.floor(i / 3) * -5;\r\n            let pos = new Laya.Vector3(x, 1.5, z);\r\n            Laya.Vector3.add(pos, road.transform.position.clone(), pos);\r\n            ball.transform.position = pos;\r\n            crl.setMyColor(arr[Math.floor(i % 3)]);\r\n            if (i == 2) {\r\n                arr = Utility.shuffleArr(arr);\r\n            }\r\n        }\r\n    }\r\n    createColorBoard(road, id) {\r\n        let cb = Laya.Sprite3D.instantiate(Utility.getSprite3DResByUrl('ColorBoard.lh'), this._levelNode, false, new Laya.Vector3(0, 0, 0));\r\n        let pos = road.transform.position.clone();\r\n        pos.y = 1;\r\n        cb.transform.position = pos;\r\n        let c = PlayerDataMgr.getFruitColor(id);\r\n        let ms = cb;\r\n        var material = new Laya.UnlitMaterial();\r\n        ms.meshRenderer.material = material;\r\n        ms.meshRenderer.sharedMaterial.albedoColor = new Laya.Vector4(c.r, c.g, c.b, c.a);\r\n        this.createColorPt(cb, id);\r\n    }\r\n    createColorPt(cb, id) {\r\n        let pt = Laya.Sprite3D.instantiate(Utility.getSprite3DResByUrl('ColorPt.lh'), this._levelNode, false, new Laya.Vector3(0, 0, 0));\r\n        let pos = cb.transform.position.clone();\r\n        pt.transform.position = pos;\r\n        let c = PlayerDataMgr.getFruitColor(id);\r\n        let ms = pt;\r\n        var material = new Laya.ShurikenParticleMaterial();\r\n        Laya.Texture2D.load(\"res/texture/Photosphere1.png\", Laya.Handler.create(this, (tex) => {\r\n            material.texture = tex;\r\n        }));\r\n        ms.particleRenderer.material = material;\r\n        ms.particleRenderer.sharedMaterial.color = new Laya.Vector4(c.r, c.g, c.b, c.a);\r\n    }\r\n    gameStart() {\r\n        Laya.Scene.open('MyScenes/GameUI.scene');\r\n        this.isStartGame = true;\r\n        this._playerCrl.playPush();\r\n    }\r\n    getDestination() {\r\n        this.isDes = true;\r\n        this.isStartGame = false;\r\n        GameUI.Share.scoreNode.visible = true;\r\n        this._playerBall.transform.translate(new Laya.Vector3(0, 0, 0.5), false);\r\n        this.moveCam1();\r\n        this._playerCrl.playKick1();\r\n        Laya.timer.once(1000, this, () => {\r\n            if (this.powerNum <= 0) {\r\n                this.kickBall(0);\r\n            }\r\n            else {\r\n                GameUI.Share.moveArrow = true;\r\n            }\r\n        });\r\n        Laya.timer.once(5000, this, () => {\r\n            if (!this.hadKicked) {\r\n                GameUI.Share.kickCB();\r\n            }\r\n        });\r\n    }\r\n    moveCam1() {\r\n        let pos = this._camera.transform.position.clone();\r\n        pos.x = this._player.transform.position.x - 7;\r\n        pos.y -= 2;\r\n        pos.z += 5;\r\n        Utility.TmoveTo(this._camera, 1000, pos, null);\r\n        Utility.RotateTo(this._camera, 1000, new Laya.Vector3(0, -110, 0), null);\r\n    }\r\n    moveCam2() {\r\n        let pos = this._camera.transform.position.clone();\r\n        pos.x = 0;\r\n        pos.y += 4;\r\n        pos.z -= 8;\r\n        Utility.TmoveTo(this._camera, 800, pos, null);\r\n        Utility.RotateTo(this._camera, 800, new Laya.Vector3(0, -180, 0), null);\r\n    }\r\n    kickBall(per) {\r\n        this.hadKicked = true;\r\n        Laya.timer.once(2000, this, () => {\r\n            this.moveCam2();\r\n            SoundMgr.instance.playSoundEffect('Kick.mp3');\r\n            this._playerBallCrl.playKickFX();\r\n            this._playerCrl.playIdle();\r\n            this._playerBallCrl.body.enabled = true;\r\n            let p = 300 * per;\r\n            p = p < 100 ? 100 : p;\r\n            this._playerBallCrl.body.applyImpulse(new Laya.Vector3(0, 0, p), null);\r\n        });\r\n        this._playerCrl.playKick2();\r\n    }\r\n    addPowerNum(v) {\r\n        this.powerNum += v;\r\n        if (this.powerNum > 12) {\r\n            this.powerNum = 12;\r\n        }\r\n        if (this.powerNum < 0) {\r\n            this.powerNum = 0;\r\n        }\r\n    }\r\n    winCB() {\r\n        SoundMgr.instance.playSoundEffect('Win.mp3');\r\n        this.isGameOver = true;\r\n        this.isWin = true;\r\n        this.isStartGame = false;\r\n        if (!this.isPause) {\r\n            this.showFinish();\r\n        }\r\n    }\r\n    showFinish() {\r\n        JJMgr.instance.openScene(SceneDir.SCENE_FULLGAMEUI, true, {\r\n            continueCallbackFun: () => {\r\n                JJMgr.instance.openScene(SceneDir.SCENE_RECOMMENDUI, true, {\r\n                    closeCallbackFun: () => {\r\n                        let s = GameUI.Share.score;\r\n                        Laya.Scene.open('MyScenes/FinishUI.scene', true, s);\r\n                    }\r\n                });\r\n            }\r\n        });\r\n    }\r\n    loseCB() {\r\n    }\r\n    activeRoad() {\r\n        if (!this._roadNode)\r\n            return;\r\n        for (let i = 0; i < this._roadNode.numChildren; i++) {\r\n            let r = this._roadNode.getChildAt(i);\r\n            if (r.transform.position.z < this._player.transform.position.z - 5) {\r\n                r.active = false;\r\n            }\r\n            else {\r\n                r.active = true;\r\n            }\r\n        }\r\n    }\r\n    restartGame() {\r\n        this.isStartGame = false;\r\n        this.isGameOver = false;\r\n        this.isWin = false;\r\n        this.powerNum = 0;\r\n        this.totalDistance = 0;\r\n        this.hadKicked = false;\r\n        this.isPause = false;\r\n        this.isDes = false;\r\n        this.colorId = 0;\r\n        this._levelNode.destroyChildren();\r\n        this._camera.transform.position = this.camStartPos;\r\n        this._camera.transform.rotation = this.camStartRotation;\r\n        this.createLevel();\r\n    }\r\n}\r\n//# sourceMappingURL=GameLogic.js.map",
