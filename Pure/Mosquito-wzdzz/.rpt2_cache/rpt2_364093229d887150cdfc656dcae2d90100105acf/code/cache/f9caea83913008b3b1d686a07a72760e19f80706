  "code": "import JJMgr from \"../Common/JJMgr\";\r\nexport default class GameIcon extends Laya.Script {\r\n    onAwake() {\r\n        this.icon = this.owner.getChildAt(0);\r\n        this.toScale(this.owner, 1, 1.1, 500, true, true);\r\n        this.initData();\r\n        Laya.timer.loop(3000, this, this.initData);\r\n    }\r\n    initData() {\r\n        var index = this.getRandomIndex();\r\n        var data = JJMgr.instance.navDataArr[index];\r\n        this.icon.skin = data.icon;\r\n        this.icon.off(Laya.Event.CLICK, this, this.navCB);\r\n        this.icon.on(Laya.Event.CLICK, this, this.navCB, [index]);\r\n    }\r\n    getRandomIndex() {\r\n        return Math.floor(Math.random() * JJMgr.instance.navDataArr.length);\r\n    }\r\n    navCB(index) {\r\n        console.log('click id:', index);\r\n        JJMgr.instance.NavigateApp(index, () => {\r\n            console.log(\"cancel\");\r\n            Laya.timer.scale = 1;\r\n        });\r\n        if (Laya.Browser.onWeiXin) {\r\n            Laya.timer.scale = 0;\r\n        }\r\n    }\r\n    toRotaion(target, angle, duration, isLoop = false, isReset = false) {\r\n        var curAngle = target.rotation;\r\n        rotate1();\r\n        function rotate1() {\r\n            if (isLoop) {\r\n                var handler = new Laya.Handler(this, rotate2);\r\n            }\r\n            else if (isReset) {\r\n                var handler = new Laya.Handler(this, rotate4);\r\n            }\r\n            Laya.Tween.to(target, { rotation: angle }, duration, null, handler);\r\n        }\r\n        function rotate2() {\r\n            Laya.Tween.to(target, { rotation: -angle }, duration * 2, null, Laya.Handler.create(this, rotate3));\r\n        }\r\n        function rotate3() {\r\n            Laya.Tween.to(target, { rotation: angle }, duration * 2, null, Laya.Handler.create(this, rotate2));\r\n        }\r\n        function rotate4() {\r\n            Laya.Tween.to(target, { rotation: curAngle }, duration);\r\n        }\r\n    }\r\n    toScale(target, scaleStart, scaleEnd, duration, isReset, isLoop = false, ease = null) {\r\n        if (!target)\r\n            return;\r\n        var handler = null;\r\n        target.scale(scaleStart, scaleStart);\r\n        scale1();\r\n        function scale1() {\r\n            handler = isLoop || isReset ? new Laya.Handler(this, scale2) : null;\r\n            Laya.Tween.to(target, { scaleX: scaleEnd, scaleY: scaleEnd }, duration, ease, handler);\r\n        }\r\n        function scale2() {\r\n            handler = isLoop ? new Laya.Handler(this, scale1) : null;\r\n            Laya.Tween.to(target, { scaleX: scaleStart, scaleY: scaleStart }, duration, ease, handler);\r\n        }\r\n    }\r\n}\r\n//# sourceMappingURL=GameIcon.js.map",
