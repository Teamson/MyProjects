      let cb = () => {\r\n            GameLogic.Share.isWin = true;\r\n            this.back();\r\n        };\r\n        ShareMgr.instance.shareGame(cb);\r\n    }\r\n    restartBtnCB() {\r\n        this.back();\r\n    }\r\n    back() {\r\n        AdMgr.instance.hideBanner();\r\n        Laya.Scene.closeAll();\r\n        JJMgr.instance.openScene(SceneDir.SCENE_FRIENDGAME, false, {\r\n            closeCallbackFun: () => {\r\n                CustomAd.instance.hideGameUIAd();\r\n                if (GameLogic.Share.isWin) {\r\n                    PlayerDataMgr.getPlayerData().grade++;\r\n                    PlayerDataMgr.setPlayerData();\r\n                }\r\n                GameLogic.Share.restartGame();\r\n                Laya.Scene.open('MyScenes/StartUI.scene', false);\r\n            }\r\n        });\r\n    }\r\n}\r\n//# sourceMappingURL=FinishUI.js.map",
